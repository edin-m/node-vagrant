{
    "config": {
        "vm": {
            "box": "ubuntu/trusty64"
        },
        "network": {
            "type": "public_network",
            "detail": {
                "guest": 83,
                "host": 85
            }
        },
        "providers": {
            "virtualbox": {
                "memory": 384
            },
            "lxc": {
                "container_name": "'test'"
            }
        },
        "provisioners": [
            {
                "type": "shell",
                "name": "shell1",
                "config": {
                    "path": "'./provision.shell.sh'"
                }
            },
            {
                "type": "ansible",
                "name": "ansible1",
                "config": {
                    "playbook": "'playbook.yml'"
                }
            },
            {
                "type": "docker",
                "name": "docker1",
                "config": {
                    "pull_images": "'ubuntu'"
                }
            },
            {
                "type": "docker",
                "name": "docker2",
                "config": {
                    "commands": [
                        "pull_images 'ubuntu'",
                        "pull_images 'ubuntu'",
                        "run 'rabbitmq'",
                        "run \"ubuntu\", cmd: \"bash -l\", args: \"-v '/vagrant:/var/www'\"",
                        "run \"db-1\", image: \"user/mysql\"",
                        "images: [\"ubuntu\", \"gentoo\"]"
                    ]
                }
            },
            {
                "type": "file",
                "name": "file1",
                "config": {
                    "source": "'./Vagrantfile'",
                    "destination": "'~/OutputVagrantfile'"
                }
            }
        ]
    }
}
